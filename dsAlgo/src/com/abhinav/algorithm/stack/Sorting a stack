public class Sort{
    public static Stack sort(Stack s){
        Stack r = new Stack();
        while(!s.isEmpty()){
            int tmp = (Integer)s.pop();
            while((!r.isEmpty() && r.top() >tmp) {
                s.push(r,pop());
                
            }
            r.push(tmp);
        }
        return r;
    }
}    
            